generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Username {
  id         Int      @id @default(autoincrement())
  username   String   @unique
  relays     String
  secretKey  String
}

model History {
  id         Int      @id @default(autoincrement())
  tweetId    String
  username   String
  timestamp  DateTime
  eventId    String   @unique
}